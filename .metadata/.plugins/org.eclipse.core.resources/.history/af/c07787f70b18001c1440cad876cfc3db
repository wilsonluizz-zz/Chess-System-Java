package application;

import chess.ChessPiece;

public class UserInterface {
	
	public static void printBoard(ChessPiece[][] pieces) {
		
	}
	
	private static void printPiece(ChessPiece piece) {
		if(piece == null) {
			System.out.print("-");
		}
		else {
			System.out.print(piece);
		}
		System.out.print(" ");
	}

}
